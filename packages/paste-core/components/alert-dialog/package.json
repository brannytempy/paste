{
  "name": "@twilio-paste/alert-dialog",
  "version": "4.0.0",
  "category": "overlay",
  "status": "production",
  "description": "An Alert Dialog is a page overlay that displays critical information, blocks interaction with the page, and only closes after an action is performed.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main": "src/index.tsx",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && NODE_ENV=production node build.js && tsc",
    "build:js": "NODE_ENV=development node build.js",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "@twilio-paste/animation-library": "workspace:*",
    "@twilio-paste/box": "workspace:*",
    "@twilio-paste/button": "workspace:*",
    "@twilio-paste/heading": "workspace:*",
    "@twilio-paste/modal": "workspace:*",
    "@twilio-paste/stack": "workspace:*",
    "@twilio-paste/style-props": "workspace:*",
    "@twilio-paste/styling-library": "workspace:*",
    "@twilio-paste/theme": "workspace:*",
    "@twilio-paste/types": "workspace:*",
    "@twilio-paste/uid-library": "workspace:*",
    "prop-types": "^15.7.2",
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  },
  "devDependencies": {
    "@twilio-paste/animation-library": "workspace:*",
    "@twilio-paste/box": "workspace:*",
    "@twilio-paste/button": "workspace:*",
    "@twilio-paste/heading": "workspace:*",
    "@twilio-paste/modal": "workspace:*",
    "@twilio-paste/stack": "workspace:*",
    "@twilio-paste/style-props": "workspace:*",
    "@twilio-paste/styling-library": "workspace:*",
    "@twilio-paste/theme": "workspace:*",
    "@twilio-paste/types": "workspace:*",
    "@twilio-paste/uid-library": "workspace:*",
    "prop-types": "^15.7.2",
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  }
}
