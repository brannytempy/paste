{
  "name": "@twilio-paste/code-block",
  "version": "0.0.0",
  "category": "data display",
  "status": "production",
  "description": "A code block is a non editable block of text formatted in the code text style and with syntax highlighting.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && NODE_ENV=production node build.js && tsc",
    "build:js": "NODE_ENV=development node build.js",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "@twilio-paste/box": "^6.0.1",
