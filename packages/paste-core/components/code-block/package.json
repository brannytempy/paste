    "react-syntax-highlighter": "^15.5.0",
    "reakit": "1.3.11",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@twilio-paste/box": "^6.0.1",
    "@twilio-paste/styling-library": "1.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-syntax-highlighter": "^15.5.0",
