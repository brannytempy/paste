{
  "name": "@twilio-paste/modal",
  "version": "11.0.1",
  "category": "overlay",
  "status": "production",
  "description": "A Modal is a page overlay that displays information and blocks interaction with the page until an action is taken or the Modal is dismissed.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main": "src/index.tsx",
  "sideEffects": false,
