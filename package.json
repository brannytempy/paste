{
  "name": "paste",
  "private": true,
  "version": "2020.05.19",
  "main": "index.js",
  "author": "Twilio Inc.",
  "license": "MIT",
  "types": "./types/index.d.ts",
  "engines": {
    "node": "^16.13.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prestart": "pnpm prebuild",
    "start": "pnpm build:tokens && concurrently \"pnpm:start:tokens\" \"pnpm:start:storybook\"",
    "start:all": "pnpm build:tokens && concurrently \"pnpm:start:tokens\" \"pnpm:start:storybook:quiet\" \"pnpm:start:website\"",
    "start:storybook": "start-storybook -s ./.storybook/static -p 9001",
    "start:storybook:quiet": "export DROP_PROGRESS=TRUE && pnpm start:storybook",
    "start:tokens": "pnpm --filter @twilio-paste/design-tokens build:watch",
    "start:website": "pnpm --filter @twilio-paste/website netlify dev ",
    "start:theme-designer": "pnpm --filter @twilio-paste/theme-designer netlify dev",
    "start:token-contrast-checker": "pnpm --filter @twilio-paste/token-contrast-checker start",
    "start:nextjs-template": "pnpm dev --filter @twilio-paste/nextjs-template",
    "start:contrast-checking": "concurrently \"pnpm:start:tokens\" \"pnpm:start:token-contrast-checker\"",
    "prebuild": "ts-node ./tools/build/pre-build.ts",
    "build": "pnpm prebuild && pnpm -r --filter=!'@twilio-paste/website' --filter=!'@twilio-paste/theme-designer' --filter=!'@twilio-paste/nextjs-template' --filter=!'@twilio-paste/token-contrast-checker' --filter=!'@twilio-paste/codemods' build",
    "build:js": "pnpm prebuild && pnpm --filter=!@twilio-paste/website --filter=!@twilio-paste/theme-designer build:js ",
    "build:codemods": "pnpm --filter @twilio-paste/codemods build ",
    "build:tokens": "pnpm --filter @twilio-paste/design-tokens tokens ",
    "build:icons": "pnpm --filter @twilio-paste/icons build ",
    "build:storybook": "build-storybook -c .storybook -o ./docs -s .storybook/static",
    "build:website": "pnpm --filter @twilio-paste/website build ",
    "build:theme-designer": "pnpm --filter @twilio-paste/theme-designer build ",
    "build:nextjs-template": "pnpm --filter @twilio-paste/nextjs-template build ",
    "pre-push": "concurrently \"pnpm lint\" \"pnpm test\" \"pnpm prettier\" \"pnpm type-check\"",
    "prerelease": "pnpm build && pnpm lint && pnpm test",
    "release": "pnpm changeset publish",
    "release:next": "pnpm lerna publish -m 'chore(release): pre release' --conventional-commits --canary --preid beta --dist-tag next",
    "version": "pnpm changeset version && pnpm i --lockfile-only",
    "sync-internal-peer-dependencies": "ts-node ./tools/build/version.ts",
    "clean": "ts-node ./tools/build/clean-repo.ts && pnpm -r clean && pnpm install",
    "clean:full": "ts-node ./tools/build/clean-repo.ts && pnpm -r clean && rm -rf node_modules/ && pnpm install",
    "clean:core": "pnpm clean --filter @twilio-paste/core",
    "pre-test": "node ./tools/build/pre-test.js",
    "test": "jest",
    "test:react-16": "USE_REACT_16=true jest",
    "test:coverage": "jest --coverage",
    "test:website": "pnpm cypress run --record",
    "test:website-gui": "WAIT_ON_TIMEOUT=600000 start-server-and-test 'pnpm start:website' http://localhost:8000 'pnpm cypress open'",
    "serve:website": "pnpm --filter @twilio-paste/website serve",
    "package-size-action-build": "pnpm build",
    "prettier": "prettier --list-different '{.storybook,packages}/**/*.{ts,tsx}'",
    "prettier-clean": "prettier --write '{.storybook,packages}/**/*.{ts,tsx}'",
    "lint": "pnpm pre-test && eslint --ext .tsx,.ts ./packages",
    "type-check": "pnpm tsc",
    "tsc": "echo 'Did you mean to run yarn type-check?'",
    "chromatic": "chromatic",
    "packages:check": "manypkg check",
    "packages:fix": "manypkg fix",
    "changeset": "changeset",
    "create:package": "plop create-package",
    "token-usage": "ts-node tools/build/token-usage-detector.ts",
    "turbo-build": "turbo run build",
    "turbo-test": "turbo run test",
    "turbo-lint": "turbo run lint",
    "turbo-type-check": "turbo run tsc"
  },
